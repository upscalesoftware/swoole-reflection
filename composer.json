{
    "name": "upscale/swoole-reflection",
    "description": "Reflection API for Swoole web-server",
    "version": "1.3.3",
    "keywords": ["Swoole", "reflection", "middleware", "headers", "body", "event", "observer", "subscription"],
    "homepage": "https://github.com/upscalesoftware/swoole-reflection",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Upscale Software Team",
            "email": "info@upscalesoftware.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-swoole": "^4.0"
    },
    "require-dev": {
        "ext-curl": "*",
        "phpunit/phpunit": "^7.0",
        "upscale/swoole-launchpad": "^1.0"
    },
    "autoload": {
        "psr-4": {"Upscale\\Swoole\\Reflection\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "Upscale\\Swoole\\Reflection\\Tests\\": "tests/",
            "Upscale\\Swoole\\Launchpad\\Tests\\": "vendor/upscale/swoole-launchpad/tests/"
        }
    }
}
